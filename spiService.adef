version: 0.1.0
sandboxed: true
start: auto

executables:
{
    spiService = (spiServiceComponent)
}

processes:
{
    envVars:
    {
        // LE_LOG_LEVEL = DEBUG
    }

    run:
    {
        (spiService)
    }

    faultAction: restart
}

requires:
{
    device:
    {
        [rw] /dev/sierra_spi /dev/
    }
}

extern:
{
    spiService.spiServiceComponent.spi
}
